{
	"name": "php-implement",
	"displayName": "php_implement",
	"description": "Implement your php interfaces/abstract classes faster",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:php-implement.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"menus": {
			"editor/context": [
				{
					"when": "editorLangId == php",
					"command": "php-implement.helloWorld"
				}
			]
		},
		"commands": [
			{
				"command": "php-implement.helloWorld",
				"title": "Implement abstract methods"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
